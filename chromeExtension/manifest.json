{
  "name": "Notification Receiver",
  "version": "0.0.1",
  "description": "Notification Receiver",
  "manifest_version": 2,
  "permissions": [
    "gcm",
    "storage",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://domtcnotifications.firebaseio.com; object-src 'self'",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_16.png",
    "default_title": "browser action",
    "default_popup": "background.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  }
}
